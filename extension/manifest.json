{
  "manifest_version": 3,
  "name": "BookmarkMakeOver",
  "version": "1.0.0",
  "description": "AI-powered bookmark organization. One credit per bookmark.",
  "permissions": ["bookmarks", "storage"],
  "host_permissions": ["http://localhost/*", "https://yourdomain.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
